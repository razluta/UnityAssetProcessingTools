                <ui:Instance template="CS_SingleSelection" name="CS_SingleSelection" style="border-top-width: 1.5px; border-bottom-width: 1.5px; border-left-color: rgb(36, 36, 36); border-right-color: rgb(36, 36, 36); border-top-color: rgb(36, 36, 36); border-bottom-color: rgb(36, 36, 36);" />
                <ui:Label text="or" name="LB_Or" style="color: rgb(190, 190, 190); -unity-text-align: upper-center; padding-left: 0; padding-right: 0; margin-left: 3px; margin-right: 3px; margin-top: 0; margin-bottom: 3px;" />
                <ui:Instance template="CS_FilterTab" name="CS_FilterTab" style="flex-grow: 1;" />
