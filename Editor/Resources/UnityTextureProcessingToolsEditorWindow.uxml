            </ui:VisualElement>
            <ui:VisualElement name="VE_ResizingTab" style="flex-grow: 1; flex-shrink: 0;">
                <ui:Instance template="CS_ResizingTab" name="CS_ResizingTab" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:Instance template="CS_Version" name="CS_Version" style="flex-shrink: 0;" />
    </ui:VisualElement>
