<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <ui:Template name="CS_FilterActive" src="CS_FilterActive.uxml" />
    <ui:Template name="BT_Tab" src="BT_Tab.uxml" />
    <ui:Template name="CS_SingleSelection" src="CS_SingleSelection.uxml" />
    <ui:Template name="CS_FilterTab" src="CS_FilterTab.uxml" />
    <ui:Template name="CS_Version" src="CS_Version.uxml" />
    <ui:Template name="CS_RenamingTab" src="CS_RenamingTab.uxml" />
    <ui:Template name="CS_ResizingTab" src="CS_ResizingTab.uxml" />
    <ui:VisualElement name="VE_Background" style="flex-grow: 1; background-color: rgb(56, 56, 56);">
        <Style src="UnityTextureProcessingToolsEditorWindow.uss" />
        <ui:Instance template="CS_FilterActive" style="flex-direction: column-reverse; flex-shrink: 0;" />
        <ui:VisualElement name="VE_Tabs" style="flex-direction: row; align-items: center; justify-content: center; margin-top: 8px; margin-bottom: 8px; background-color: rgb(56, 56, 56); flex-shrink: 0;">
            <ui:Instance template="BT_Tab" name="BT_FilterTab" style="display: flex;" />
            <ui:Instance template="BT_Tab" name="BT_RenamingTab" />
            <ui:Instance template="BT_Tab" name="BT_ResizingTab" style="display: flex;" />
        </ui:VisualElement>
        <ui:VisualElement name="VE_BigContainer" style="flex-grow: 1; flex-shrink: 0;">
